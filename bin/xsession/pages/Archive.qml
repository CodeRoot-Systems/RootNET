                PropertyChanges { target: verticalScrollBar; opacity: 1 }
            }

            transitions: Transition {
                NumberAnimation { target: verticalScrollBar; properties: "opacity"; duration: 400 }
            }

            Column {
                id: objList
                spacing: 2
                height: archive.length*60
                anchors {
                    top: parent.top
                    left: parent.left
                    right: parent.right
                }

                Repeater {
                    id: items
                    model: content
                    delegate:

                        Item {
                           height: 65
                           anchors {left: parent.left; right: parent.right}

                           states: [
                               State {
                                   name: "Focused"
                                   PropertyChanges {
                                       target: details
                                       height: objName.height + objDetails.height + 5
                                   }
                                   PropertyChanges {
                                       target: downloader
                                       opacity: 1.0
                                   }
                                }
                           ]

                           transitions: [
                               Transition {
                                   NumberAnimation {
                                       target: details
                                       property: "height"
                                       duration: 200
                                       easing.type: Easing.InOutQuad
                                   }
                               },
                               Transition {

                                   NumberAnimation {
                                       target: downloader
                                       property: "opacity"
                                       duration: 1000
                                       easing.type: Easing.InOutQuad
                                   }
                               }
                           ]

                           MouseArea {
                               anchors.fill: parent
                               hoverEnabled: true
                               onPressed: parent.state = 'Focused'
                               onEntered:  parent.state = 'Focused'
                               onExited:  parent.state = ''

                               Image {
                                   id: thumbnail
                                   source: "../thumbs/appbar.folder.lock.png"
                                   anchors {verticalCenter: parent.verticalCenter}
                                   asynchronous: true
                               }

                               Rectangle {
                                   id: details
                                   clip: true
                                   height: objName.height
                                   anchors {verticalCenter: parent.verticalCenter; left: thumbnail.right; right: downloader.left}
                                   color: 'transparent'

                                   Text {
                                       id: objName
                                       text: modelData.Name
                                       color: "black"
                                       font {family: "Arial"; pixelSize: 14}
                                       anchors {top: parent.top; left: parent.left; right: parent.right}
                                   }

                                   Details {
                                       id: objDetails
                                       height: objData.length * 15
                                       anchors {top: objName.bottom; topMargin: 5; left: parent.left; right: parent.right }
                                       vlColor: 'darkblue'
                                       objData: [
                                           {title: 'Hash: ', value: modelData.Hash},
                                           {title: 'Size (Bytes): ', value: modelData.Size}
                                       ]
                                   }
                               }

                               Rectangle {
                                   id: downloader
                                   width: 30; height: 30; radius: width/0.5
                                   opacity: 0.0
                                   color: 'black'
                                   scale: 1
                                   anchors {verticalCenter: parent.verticalCenter; right: parent.right; rightMargin: 15}

                                   MouseArea {
                                       anchors.fill: parent
                                       hoverEnabled: true
                                       cursorShape: Qt.PointingHandCursor
                                       onPressed: parent.scale = 0.87
                                       onReleased: parent.scale = 1

                                       Image {
                                           source: '../thumbs/appbar.download.png'
                                           height: 28; width: 30
                                           asynchronous: true
                                           anchors.centerIn: parent
                                       }
                                   }
                               }

                               Rectangle {
                                   id: border
                                   height: 1
                                   color: 'Gray'
                                   anchors{bottom: parent.bottom; left: parent.left; right: parent.right}
                               }
                            }
                        }
                }
            }
        }

        ScrollBar {
            id: verticalScrollBar
            width: 12; height: view.height
            anchors.right: view.right
            opacity: 0
            orientation: Qt.Vertical
            position: view.visibleArea.yPosition
            pageSize: view.visibleArea.heightRatio
        }
    }
}
